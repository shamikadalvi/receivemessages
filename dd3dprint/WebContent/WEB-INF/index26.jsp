          }

       
        


}


void serialEvent() {
  while (Serial.available()) {
    // get the new byte:
    char inChar = (char)Serial.read();
    // add it to the inputString:
    inputString += inChar;
   
    if (inChar == '#') {
      
      stringComplete = true;
       contactflag=1;
    
    }
  }
}

  
