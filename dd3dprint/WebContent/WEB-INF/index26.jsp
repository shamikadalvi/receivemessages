void adc_read()
{
int i,j,read_no;

  unsigned long adc_val;
  data_read=digitalRead(adc_data_pin);

 
             digitalWrite(clk,0);
              
   
             while(data_read==1);

            
                 read_no=0;
                 adc_val=0;
              digitalWrite(clk,1);
                adc_val=adc_val<<1;
               digitalWrite(clk,0);

                if(data_read)

            {
                        adc_val++;
                        read_no++;


                    if(read_no>=26)
                    {

                     digitalWrite(clk,1);
                    adc_avg=(double)adc_val;
                      digitalWrite(clk,0);
                     }
            }

   }
   
   
