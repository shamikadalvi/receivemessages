	   System.out.println("reached here 2");
	   oledWriteCmd(SSD1306_DISPLAY_RESET); 
	   System.out.println("reached here 3");
	   oledWriteCmd(SSD1306_SET_DISPLAY_OFFSET);
	   System.out.println("reached here 4");
